    </string-array>

    <string-array name="rp_recur_formats">
        <item>Does not repeat</item>
        <item>Repeats every {0,choice,1#day|1&lt;{0} days}</item> <!-- Daily, {0}=number of days -->
        <item>Repeats every {0,choice,1#week|1&lt;{0} weeks}{1,choice,0#|1# on {2}}</item> <!-- Weekly, {0}=number of weeks, {1}=whether to show days list (yes=1), {2}=days of week list -->
        <item>Repeats every {0,choice,1#month|1&lt;{0} months}{1,choice,0#|1# ({2})}</item> <!-- Monthly, {0}=number of months, {1}=whether to show extra setting (yes=1), {2}=repeat on same day of month/on every nth day of week -->
        <item>Repeats every {0,choice,1#year|1&lt;{0} years}</item> <!-- Yearly, {0}=number of years -->
    </string-array>

    <string-array name="rp_end_formats">
        <item></item> <!-- Forever -->
        <item>until {0}</item> <!-- Until a date -->
        <item>for {0,choice,1#one event|1&lt;{0} events}</item> <!-- For a number of events -->
        <item>until {0} or for {1,choice,1#one event|1&lt;{1} events}</item> <!-- Until a date or for a number of events, the one that comes first -->
    </string-array>

    <!-- Separator between each days of week when listing them
         Ex: Sun, Tue, Fri, Sat
         Use \u0020 for regular space, trailing whitespace get removed -->
    <string name="rp_days_of_week_list_sep">,\u0020</string>

    <!-- Instead of listing all days when all of them are check, use this
         This only happens if frequency > 1 otherwise it gets converted to a daily recurrence -->
    <string name="rp_days_of_week_list_all">every day of the week</string>

    <!-- How to concatenate recurrence and end date/count when formatting -->
    <string name="rp_merge_format">{0}; {1}</string>

